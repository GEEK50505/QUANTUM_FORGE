{
    "rooscode.proxyEnabled": true,
    "rooscode.automaticStartup": true,
    "rooscode.enableAutoKeyRotation": true,
    // Continue extension configuration
    "continue.provider": "openrouter",
    "continue.openrouter.model": "qwen-3-coder-free",
    // Editor settings
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true
    },
    "python.analysis.extraPaths": [
        "${workspaceFolder}"
    ]
}